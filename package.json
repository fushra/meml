{
  "name": "meml",
  "version": "1.0.0-b",
  "description": "A basic meml interpreter written and tested in typescript",
  "main": "./dist/index.js",
  "homepage": "https://meml.fivnex.co",
  "bin": {
    "memlc": "./dist/app.js"
  },
  "repository": "https://github.com/fushra/meml.git",
  "author": "TrickyPR <trickypr@icloud.com>",
  "contributors": [
    {
      "name": "Kai Lyons",
      "email": "kai@fivnex.co",
      "url": "https://github.com/kaitheceo"
    }
  ],
  "license": "GPL-v3-only",
  "private": false,
  "scripts": {
    "test": "nyc --reporter=lcov --reporter=html testyts",
    "test:dev": "testyts",
    "ast": "ts-node ./scripts/GenerateAst.ts && prettier . --write",
    "app": "ts-node ./src/app.ts",
    "build": "tsc && echo '#!/usr/bin/env node\n' >  tmpfile && cat ./dist/app.js >> tmpfile && mv tmpfile ./dist/app.js",
    "examples": "yarn app --file ./css.meml --file ./helloWorld.meml --src ./examples --out ./examples"
  },
  "devDependencies": {
    "@types/node": "^14.14.44",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0",
    "ts-node": "^9.1.1",
    "testyts": "^1.1.2",
    "typescript": "^4.2.3",
    "yarn": "^1.22.10"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "cmd-ts": "^0.6.9",
    "is-browser": "^2.1.0",
    "memfs": "^3.2.2",
    "path.js": "^1.0.7"
  }
}
