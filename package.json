{
  "name": "meml",
  "version": "1.0.0",
  "description": "A basic meml interpreter written and tested in typescript",
  "main": "./dist/core.js",
  "bin": {
    "memlc": "./dist/app.js"
  },
  "repository": "https://github.com/fushra/meml.git",
  "author": "TrickyPR <trickypr@icloud.com>",
  "license": "GPL-v3-only",
  "private": true,
  "scripts": {
    "test": "testyts",
    "ast": "ts-node ./scripts/GenerateAst.ts && prettier . --write",
    "app": "ts-node ./src/app.ts",
    "build": "tsc && echo '#!/usr/bin/env node\n' >  tmpfile && cat ./dist/app.js >> tmpfile && mv tmpfile ./dist/app.js",
    "examples": "yarn app --file ./css.meml --file ./helloWorld.meml --src ./examples --out ./examples"
  },
  "devDependencies": {
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1"
  },
  "dependencies": {
    "cmd-ts": "^0.6.9",
    "@types/node": "^14.14.36",
    "testyts": "^1.1.2",
    "chalk": "^4.1.1",
    "typescript": "^4.2.3"
  }
}
